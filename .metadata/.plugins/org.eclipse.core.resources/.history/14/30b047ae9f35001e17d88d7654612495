package vehicle;

import move.Action;

public class Bicycle implements Action {
	String name;
	Handle hd;
	Wheel wh;
	@Override
	public void go() {
		System.out.println("자전거가 간다");
	}
	@Override
	public void repair() {
		System.out.println("자전거를 고친다");
	}
	@Override
	public void fall() {
		System.out.println("자전거가 쓰러진다");
	}
}
