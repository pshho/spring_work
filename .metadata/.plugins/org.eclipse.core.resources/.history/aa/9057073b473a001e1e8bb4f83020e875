package coffeeAssignment.coffeeKinds;

import coffeeAssignment.Coffee;

public class Americano implements Coffee {
	String kinds = "콜롬비아";
	int coffeeBeans = 70;
	int water = 180;
	int price = 2000;
	
	@Override
	public String toString() {
		return "Americano [kinds=" + kinds + ", coffeeBeans=" + coffeeBeans + ", water=" + water + ", price=" + price
				+ "]";
	}
}
