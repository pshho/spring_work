package mobile;

import java.util.List;

public class SmartPhone {
	String name;
	List<Camera> cam;
	Display disp;
	Battery bat;
	
	public void setName(String name) {
		this.name = name;
	}
	public void setCam(List<Camera> cam) {
		this.cam = cam;
	}
	public void setDisp(Display disp) {
		this.disp = disp;
	}
	public void setBat(Battery bat) {
		this.bat = bat;
	}
}
