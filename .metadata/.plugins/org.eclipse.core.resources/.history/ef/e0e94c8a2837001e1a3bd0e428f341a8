package diet;

public class Lunch {
	String rice, sideDish, mainDish, soup;

	public Lunch(String rice, String sideDish, String mainDish, String soup) {
		super();
		this.rice = rice;
		this.sideDish = sideDish;
		this.mainDish = mainDish;
		this.soup = soup;
	}

	@Override
	public String toString() {
		return "Lunch [rice=" + rice + ", sideDish=" + sideDish + ", mainDish=" + mainDish + ", soup=" + soup + "]";
	}
	
}
