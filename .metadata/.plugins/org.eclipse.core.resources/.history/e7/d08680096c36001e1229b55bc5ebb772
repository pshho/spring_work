package schools;

import java.util.Arrays;
import java.util.Iterator;
import java.util.Map;

public class Student {
	String name;
	Map<String, Integer> subjects;
	int sum, rank;
	double avg;
	
	public Student(String name, Map<String, Integer> subjects) {
		this.name = name;
		this.subjects = subjects;
	}
	public String getRank() {
		return "난 "+name+"인데 "+rank+"등이야..";
	}
	void calc(Map<String, Integer> subjects) {
		
	}
	@Override
	public String toString() {
		return "Student [name=" + name + ", subjects=" + Arrays.toString(subjects) + ", sum=" + sum + ", rank=" + rank
				+ ", avg=" + avg + "]";
	}
}
