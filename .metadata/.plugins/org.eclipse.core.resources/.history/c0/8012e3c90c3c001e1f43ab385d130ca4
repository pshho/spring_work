package figure.parts;

public class Rectangle {
	int width, height;
	public int area, perimeter;
	
	public Rectangle(int width, int height) {
		super();
		this.width = width;
		this.height = height;
	}

	public void setWidth(int width) {
		this.width = width;
	}

	public void setPerimeter(int perimeter) {
		this.perimeter = perimeter;
	}
}
