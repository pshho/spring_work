package bicycleComponent;

import move.Action;

public class Bicycle implements Action {
	String name;
	Handle hd;
	Wheel[] whs;
	Basket bk;
	@Override
	public void go() {
		System.out.println(name+"가 간다");
	}
	@Override
	public void repair() {
		System.out.println(name+"를 고친다");
	}
	@Override
	public void fall() {
		System.out.println(name+"가 쓰러진다");
	}
}
