package diet;

public class FoodMenu {
	String rice, soup, side, sideDish, toast, fruit;
	
	public FoodMenu() {
		super();
	}

	public FoodMenu(String rice, String soup, String side, String sideDish) {
		super();
		this.rice = rice;
		this.soup = soup;
		this.side = side;
		this.sideDish = sideDish;
		System.out.println("우리 모두 맛있는 저녁을 먹어요\n오늘의 저녁 식단은");
	}

	@Override
	public String toString() {
		return "DinnerMenu [rice=" + rice + ", soup=" + soup + ", side=" + side + ", sideDish=" + sideDish + "]";
	}
}
