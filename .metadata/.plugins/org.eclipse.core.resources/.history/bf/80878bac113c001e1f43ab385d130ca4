package figure.parts;

public class Rectangle {
	public int area, perimeter;
	
	public Rectangle() {
		super();
	}

	public int rectArea(int width, int height) {
		area = width*height;
		return area;
	}
	
	public int rectPerimeter(int width, int height) {
		perimeter = (width + height) * 2; 
		return perimeter;
	}
}
