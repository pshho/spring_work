package schools;

import java.util.Iterator;
import java.util.Map;
import java.util.Map.Entry;

public class Student {
	String name;
	Map<String, Integer> subjects;
	int sum, rank;
	double avg;
	
	public Student(String name, Map<String, Integer> subjects) {
		this.name = name;
		this.subjects = subjects;
	}
	public String getRank() {
		return "난 "+name+"인데 "+rank+"등이야..";
	}
	void calc() {
		Iterator<Entry<String, Integer>> values = subjects.entrySet().iterator();
		
	}
	@Override
	public String toString() {
		return "Student [name=" + name + ", subjects=" + subjects + ", sum=" + sum + ", rank=" + rank + ", avg=" + avg
				+ "]";
	}
}
