package figure;

import org.springframework.stereotype.Component;

import figure.parts.Circle;
import figure.parts.Rectangle;
import figure.parts.RightTriangle;

@Component
public class Figure {
	public Rectangle rect;
	public RightTriangle rigTri;
	public Circle circle;

	public Figure() {
		super();
		this.rect = new Rectangle();
		this.rigTri = new RightTriangle();
		this.circle = new Circle();
	}

}
