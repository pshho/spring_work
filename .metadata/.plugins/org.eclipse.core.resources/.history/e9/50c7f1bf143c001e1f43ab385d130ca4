package figure.parts;

public class Rectangle {
	public int rectArea, rectPerimeter;
	
	public Rectangle() {
		super();
	}

	public int areaRect(int width, int height) {
		rectArea = width*height;
		return rectArea;
	}
	
	public int perimeterRect(int width, int height) {
		rectPerimeter = (width + height) * 2; 
		return rectPerimeter;
	}
}
