package studentAop;

import java.util.HashMap;

public class Students {
	HashMap<String, Integer> subjects = new HashMap<>();
	
	public Student std1() {
		subjects.put("국어", 77);
		subjects.put("수학", 89);
		return new Student("일", subjects);
	}
	
	public Student std2() {
		subjects.put("국어", 57);
		subjects.put("수학", 61);
		return new Student("이", subjects);
	}
	
	public Student std3() {
		subjects.put("국어", 34);
		subjects.put("수학", 46);
		subjects.put("영어", 98);
		return new Student("삼", subjects);
	}
	
	public Student std4() {
		subjects.put("국어", 73);
		subjects.put("수학", 55);
		subjects.put("영어", 82);
		return new Student("삼", subjects);
	}
}
