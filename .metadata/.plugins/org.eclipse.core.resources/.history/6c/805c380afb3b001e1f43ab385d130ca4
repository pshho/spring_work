package aop_p.webToon;

import java.util.HashMap;

public class KangBada {
	HashMap<String, KangFish> map;
	
	public KangBada() {
		map = new HashMap<>();
		
		map.put("지렁이", new KangFish("붕어", 30));
		map.put("새우", new KangFish("잉어", 10));
		map.put("떡밥", new KangFish("인어", 20));
		map.put("오징어", new KangFish("어인", 150));
	}

	public KangFish fishing(String lure) {
		System.out.println(lure + "로 낚시해요");
		return null;
	}
}
