package aop_p.webToon;

import org.springframework.stereotype.Component;

@Component
public class KangFish {
	public String name;
	public int size;
	
	public KangFish() {
		super();
	}

	public KangFish(String name, int size) {
		super();
		this.name = name;
		this.size = size;
	}

	@Override
	public String toString() {
		return "KangFish [name=" + name + ", size=" + size + "]";
	}
}
