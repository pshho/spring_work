package bicycleComponent;

import org.springframework.context.support.AbstractApplicationContext;
import org.springframework.context.support.ClassPathXmlApplicationContext;

public class BicycleMove {

	public static void main(String[] args) {
		AbstractApplicationContext context =
				new ClassPathXmlApplicationContext("di_xml/bicycle.xml");
		// Bicycle by1 = (Bicycle)context.getBean("by1");
		Bicycle by2 = (Bicycle)context.getBean("by2");
		Bicycle by3 = (Bicycle)context.getBean("by3");
		// System.out.println(by1);
		System.out.println(by2);
		System.out.println(by3);
	}

}
