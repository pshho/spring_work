package diet;

public class DinnerMenu {
	String rice, soup, side, sideDish;
	
	public DinnerMenu() {
		super();
	}

	public DinnerMenu(String rice, String soup, String side, String sideDish) {
		super();
		this.rice = rice;
		this.soup = soup;
		this.side = side;
		this.sideDish = sideDish;
	}

	@Override
	public String toString() {
		return "DinnerMenu [rice=" + rice + ", soup=" + soup + ", side=" + side + ", sideDish=" + sideDish + "]";
	}
}
