package bicycleComponent;

import move.Action;

public class Bicycle implements Action {
	String name;
	Handle hd;
	Wheel wh;
	Basket bk;
	public Basket getBk() {
		return bk;
	}
	public void setBk(Basket bk) {
		this.bk = bk;
	}
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	public Handle getHd() {
		return hd;
	}
	public void setHd(Handle hd) {
		this.hd = hd;
	}
	public Wheel getWh() {
		return wh;
	}
	public void setWh(Wheel wh) {
		this.wh = wh;
	}
	@Override
	public String toString() {
		return "Bicycle [name=" + name + ", hd=" + hd + ", wh=" + wh + "]";
	}
	@Override
	public void go() {
		System.out.println("자전거가 간다");
	}
	@Override
	public void repair() {
		System.out.println("자전거를 고친다");
	}
	@Override
	public void fall() {
		System.out.println("자전거가 쓰러진다");
	}
}
